# Alfred Task Manager Environment Variables
# Copy this file to .env and set your API keys

# OpenAI API Key (required for AI provider functionality)
OPENAI_API_KEY=your_openai_api_key_here

# Google/Gemini API Key (get from https://aistudio.google.com)
GOOGLE_API_KEY=your_google_api_key_here

# Anthropic API Key (get from https://console.anthropic.com)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Debug settings
ALFRED_DEBUGGING_MODE=true

ATLASSIAN_CLOUD_ID=your_atlassian_cloud_id_here
ATLASSIAN_API_TOKEN=your_atlassian_api_token_here
ATLASSIAN_EMAIL=your_atlassian_email_here